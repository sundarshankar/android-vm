[project]
	description = Automated provisioning and configuration of an Ubuntu VM containing the Android development environment, including Android ADT Bundle with SDK, Eclipse & the Android NDK using the Vagrant DevOps tool with Chef and shell-scripts.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/androiddots (sundarshankar)
[access "refs/for/refs/*"]
	read = group user/androiddots (sundarshankar)
	push = group user/androiddots (sundarshankar)
	label-Code-Review = -2..+2 group user/androiddots (sundarshankar)
	label-Verified = -1..+1 group user/androiddots (sundarshankar)
	rebase = group user/androiddots (sundarshankar)
	removeReviewer = group user/androiddots (sundarshankar)
	submit = group user/androiddots (sundarshankar)
[access "refs/heads/*"]
	read = group user/androiddots (sundarshankar)
	create = group user/androiddots (sundarshankar)
	push = +force group user/androiddots (sundarshankar)
	pushMerge = group user/androiddots (sundarshankar)
[access "refs/tags/*"]
	push = +force group user/androiddots (sundarshankar)
	pushTag = group user/androiddots (sundarshankar)
	pushSignedTag = group user/androiddots (sundarshankar)
